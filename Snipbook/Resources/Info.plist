<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>$(DEVELOPMENT_LANGUAGE)</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>$(PRODUCT_NAME)</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>1.0</string>
	<key>CFBundleVersion</key>
	<string>1</string>
	<key>NSCameraUsageDescription</key>
	<string>Snipbook uses your camera to capture photos for your collection.</string>
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>Snipbook saves where you captured each snip so you can remember the location.</string>
	<key>NSPhotoLibraryAddUsageDescription</key>
	<string>Snipbook can save your snips to your photo library.</string>
	<key>NSPhotoLibraryUsageDescription</key>
	<string>Snipbook needs access to your photo library to import images.</string>
	<key>UIAppFonts</key>
	<array>
		<string>Lexend-Regular.ttf</string>
		<string>Lexend-Medium.ttf</string>
		<string>Pacifico-Regular.ttf</string>
	</array>
	<key>UILaunchScreen</key>
	<dict/>
</dict>
</plist>
